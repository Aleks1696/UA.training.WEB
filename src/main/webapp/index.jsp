
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
  <head>
    <title>Registration</title>
  </head>
  <body>
    <fieldset>
      <legend>Authorization form</legend>
      <form method="post" action="submit">
        <h1>Enter login: </h1>
        <input type="text" placeholder="Michael" name="login"/>
        <h1>Enter e-mail: </h1>
        <input type="email" placeholder="mike@gmail.com" name="email"/>
        <input type="submit" value="Submit"/>
      </form>
    </fieldset>
  </body>
</html>
